{
  "name": "freezed-bloc",
  "displayName": "freezed_bloc",
  "description": "support for create bloc classes with freezed packages",
  "version": "0.2.2",
  "engines": {
    "vscode": "^1.46.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/QAQyzk/freezed_bloc_extension"
  },
  "publisher": "QAQyzk",
  "license": "",
  "categories": [
    "Snippets",
    "Other"
  ],
  "keywords": [
    "dart",
    "bloc",
    "freezed"
  ],
  "activationEvents": [
    "workspaceContains:**/pubspec.yaml",
    "onCommand:extension.createFreezedBloc"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "snippets": [
      {
        "language": "dart",
        "path": "./snippet/dart_snippet.json"
      }
    ],
    "commands": [
      {
        "command": "extension.createFreezedBloc",
        "title": "Create freezed bloc"
      },
      {
        "command": "extension.createNewBlocEvent",
        "title": "Freezed Bloc: Create new bloc event"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "true",
          "command": "extension.createFreezedBloc",
          "group": "freezedBloc@1"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.46.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "eslint": "^6.8.0",
    "@typescript-eslint/parser": "^2.30.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  }
}
